}

instance Root.Framework.N1.L1

device D {
	<** @eaguid:'{abcd-x-x-x-fc}' **>
	hosts org.example.Root.Framework.N1.L1
	
	instance Root.Framework.N2.L1 
	instance Root.Framework.N2.L2
	instance Root.Framework.N2.L3
}